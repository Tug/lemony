{
	"name": "@diversifiedfinance/web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"predev": "docker ps && docker-compose --file docker-compose.dev.yaml up cockroachdb --no-recreate --detach",
		"dev": "STAGE=development NODE_OPTIONS='--inspect' next dev",
		"docker": "docker-compose --file docker-compose.dev.yaml",
		"build": "STAGE=production next build",
		"build:turbo": "cd ../.. && yarn build",
		"build:analyze": "ANALYZE=true yarn build",
		"postbuild": "bash scripts/force-boot-endpoints.sh",
		"vercel-build": "prisma generate && prisma migrate deploy && next build",
		"start": "next start",
		"build:dev": "dotenv -e .env.development yarn build",
		"prisma:dev": "dotenv -e .env.development -- prisma",
		"prisma-format": "yarn prisma format",
		"migrate:dev": "yarn prisma:dev migrate dev",
		"seed:dev": "yarn prisma:dev db seed && yarn ts-node:dev ./scripts/db-sync.ts",
		"migrate:resolve:prod": "dotenv -e .env.production.local -- prisma migrate resolve",
		"studio": "dotenv -e .env.development npx prisma studio",
		"prisma:test": "dotenv -e .env.test -- prisma",
		"seed:test": "yarn prisma:test db seed",
		"pretest:reset": "docker-compose --file docker-compose.test.yaml down --volumes && docker-compose --file docker-compose.test.yaml up --detach && ./scripts/db-wait.sh localhost 26258 && yarn migrate:test",
		"test:reset": "yarn test:integration",
		"pretest": "yarn prisma:test generate",
		"test": "yarn test:integration",
		"migrate:test": "dotenv -e .env.test -- prisma migrate reset --force",
		"test:integration": "jest --config ./jest.config.js -i",
		"ts-node:dev": "dotenv -e .env.development -- npx ts-node",
		"ts-node:test": "dotenv -e .env.test -- npx ts-node",
		"ts-node:production:emergency": "dotenv -e .env.production.local -- npx ts-node",
		"dev:production:emergency": "dotenv -e .env.production.local -- next dev",
		"dev:production:vercel-build": "dotenv -e .env.production.local -- yarn vercel-build",
		"pretest-ci": "dotenv -e .env.ci -- prisma migrate reset --force",
		"test-ci": "dotenv -e .env.ci -- yarn test:integration",
		"locize:scan": "i18next-scanner",
		"prelocize:sync": "yarn locize:scan",
		"locize:sync": "cd public/i18n && ln -s ../../.env.development .env && locize sync && rm .env"
	},
	"prisma": {
		"seed": "ts-node scripts/db-seed.ts"
	},
