  {
  "name": "lemony",
  "version": "15.4.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/react": "~18.2.14",
    "@diversifiedfinance/eslint-plugin": "*",
    "@diversifiedfinance/prettier-config": "*",
    "@diversifiedfinance/babel-preset-default": "*",
    "@diversifiedfinance/browserslist-config": "*",
    "@diversifiedfinance/tsconfig": "*",
    "eslint": "^8.44.0",
    "husky": "^8.0.1",
    "patch-package": "^7.0.1",
    "lint-staged": "^13.2.3",
    "react-native-os": "^1.2.6",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@expo/config-plugins": "~7.2.2",
    "babel-loader": "^9.1.3",
    "babel-preset-expo": "9.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@jest/create-cache-key-function": "29.2.1",
    "jest-runtime": "29.5.0",
    "@types/react": "18.2.14",
    "@ethersproject/bignumber": "5.7.0",
    "@ethersproject/signing-key": "5.7.0",
    "react-native-mmkv": "2.10.1",
    "react-native-screens": "3.27.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "native": "cd apps/expo && npx expo start",
    "native:clear": "yarn cleanup; yarn; cd apps/expo && npx expo start --clear",
    "web": "cd apps/web && yarn next",
    "prettier-check": "prettier --check ./packages ./apps",
    "prettier-fix": "prettier --write ./packages ./apps",
    "refresh": "yarn cleanup && yarn",
    "afresh": "rm yarn.lock && yarn cleanup && yarn",
    "cleanup": "yarn cleanup-cache; yarn cleanup-dependencies;",
    "cleanup-cache": "rm -rf apps/web/.expo && rm -rf apps/web/.next && watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn cache clean",
    "cleanup-dependencies": "rm -rf apps/expo/node_modules; rm -rf apps/web/node_modules; rm -rf packages/app/node_modules; rm -rf node_modules;",
    "pre:install-rn-dependency": "yarn install-expo-dependency",
    "install-rn-dependency": "yarn install-ui-dependency",
    "post:install-rn-dependency": "yarn cleanup && yarn",
    "install-expo-dependency": "cd apps/expo && npx expo install --",
    "install-ui-dependency": "cd packages/app && yarn add --",
    "upgrade:design-system": "yarn up '@diversifiedfinance/design-system/*'",
    "dev": "yarn turbo run dev --parallel --no-cache",
    "dev:native": "cd apps/expo && yarn start:dev-client:clear",
    "dev:expo": "yarn turbo run dev --parallel --no-cache --scope=@diversifiedfinance/expo",
    "dev:next": "yarn turbo run dev --parallel --no-cache --scope=@diversifiedfinance/web",
    "dev:next-storybook": "yarn turbo run dev --parallel --no-cache --scope=@diversifiedfinance/storybook-react",
    "dev:web": "yarn turbo run dev --parallel --no-cache --scope=@diversifiedfinance/next --scope=@diversifiedfinance/storybook-react",
    "dev:mobile": "yarn turbo run dev --parallel --no-cache --scope=@diversifiedfinance/expo",
    "build": "yarn turbo run build",
    "build:expo": "yarn turbo run build --scope=@diversifiedfinance/expo",
    "build:next": "yarn turbo run build --scope=@diversifiedfinance/web",
    "build:next-storybook": "yarn turbo run build --scope=@diversifiedfinance/storybook-react",
    "start:next": "yarn turbo run start --scope=@diversifiedfinance/web",
    "lint": "yarn turbo run lint",
    "lint:fix": "yarn turbo run lint:fix",
    "lint:report": "yarn turbo run lint:report",
    "graph": "yarn turbo:build --graph",
    "graph:next": "yarn turbo:build:next --graph",
    "yolo": "yarn turbo:dev:next & yarn workspace @diversifiedfinance/expo ios & yarn workspace @diversifiedfinance/expo android",
    "clean": "yarn clean:turbo & yarn clean:node-modules & yarn clean:build",
    "clean:turbo": "rm -rf ./node_modules/.cache/turbo",
    "clean:build": "rm -rf apps/.next",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "postinstall": "patch-package && husky install",
    "clean:native": "yarn clean && rm -rf apps/expo/android && rm -rf apps/expo/ios && rm -rf apps/expo/.expo",
    "test:expo": "cd apps/expo && yarn test",
    "test": "jest"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "dns": "dns.js",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css}": "eslint",
    "apps/**/*.{js,jsx,ts,tsx,json,css}": "eslint"
  },
  "browser": {
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "_stream_readable": "readable-stream/readable",
    "_stream_transform": "readable-stream/transform",
    "_stream_writable": "readable-stream/writable",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "dgram": "react-native-udp",
    "dns": "dns.js",
    "domain": "domain-browser",
    "fs": "react-native-level-fs",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tls": false,
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "zlib": "browserify-zlib"
  }
}
